{
  "name": "star-orgs",
  "private": true,
  "version": "0.0.0",
  "description": "Azure Active Directory Organization Chart Viewer",
  "main": "server.js",
  "keywords": [
    "azure",
    "active directory",
    "ad"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ritterim/star-orgs"
  },
  "scripts": {
    "audit": "nsp check package",
    "build": "npm run build-client && npm run build-server",
    "build-client": "webpack",
    "build-client-production": "cross-env NODE_ENV=production webpack",
    "build-server": "babel src/server -d dist",
    "build-server-and-start-server": "npm run build-server && node ./server.js",
    "check": "npm run audit && npm outdated --depth 0",
    "clean:keep-env": "git clean -xdf --exclude .env",
    "coverage": "jest --coverage --forceExit",
    "lint": "eslint src tests",
    "start": "node server.js",
    "start:watch": "concurrently \"npm run watch-client\" \"npm run watch-server\"",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "validate": "npm run lint && npm test",
    "watch-client": "webpack --watch",
    "watch-server": "nodemon --exec npm run build-server-and-start-server --watch src/server --watch .env"
  },
  "author": {
    "name": "Ritter Insurance Marketing"
  },
  "pre-commit": [
    "validate"
  ],
  "dependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "7.0.0-beta.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.0",
    "cache-storage": "^2.0.1",
    "chance": "^1.0.6",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "d3-force": "^1.0.6",
    "d3-scale": "^1.0.5",
    "d3-selection": "^1.0.5",
    "d3-svg-legend": "^2.24.0",
    "d3-transition": "^1.0.4",
    "dotenv": "^4.0.0",
    "eventemitter3": "^2.0.3",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "md5": "^2.2.1",
    "node-sass": "^4.5.2",
    "randgen": "^0.1.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "whatwg-fetch": "^2.0.3",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "concurrently": "^3.4.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "fetch-mock": "^5.9.4",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "nsp": "^2.6.3",
    "pre-commit": "^1.2.2",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">=6 <8",
    "npm": ">=3"
  }
}
